{
    "name": "bencavens/chief",
    "description": "A blog engine for Laravel site environments",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "cartalyst/sentry": "2.1.*",
        "bencavens/core": "0.1.*"
    },
    "require-dev":{
        "fzaninotto/faker": "1.3.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/seeds",
            "src/controllers"
        ],
        "psr-0": {
            "Bencavens\\Chief\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
